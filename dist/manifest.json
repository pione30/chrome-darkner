{
  "manifest_version": 2,

  "name": "Chrome Darkener",
  "description": "Darken current page",
  "version": "1.0",
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Chrome Darkener"
  },
  "permissions": [
    "activeTab"
  ]
}
